{
  "name": "@tfk-samf/components",
  "version": "0.2.0",
  "description": "Web components used for applications in Troms County.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/components.umd.cjs",
  "module": "./dist/components.js",
  "exports": {
    ".": {
      "import": "./dist/components.js",
      "require": "./dist/components.umd.cjs"
    },
    "./style": "./dist/style.css",
    "./vuetify": "./dist/vuetify/svipper-theme.ts"
  },
  "eslintConfig": {
    "extends": "../../.eslintrc"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && copyfiles -u 1 ./src/vuetify/svipper-theme.ts dist",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx --fix ."
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  },
  "peerDependencies": {
    "@tfk-samf/tokens": "0.2.0",
    "vuetify": "^3.6.11"
  },
  "gitHead": "f2a3afa0b479e55c1b4524a35c9418303c7a0fe4",
  "dependencies": {
    "@shoelace-style/localize": "^3.2.1",
    "@shoelace-style/shoelace": "^2.15.1"
  }
}
